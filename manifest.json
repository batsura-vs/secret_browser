{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html",
    "default_title": "@#!3"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "aes.js"
  ],
  "manifest_version": 2,
  "name": "Voven4ek",
  "version": "0.0.0.1",
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  }
}
